<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/page11_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:src="@drawable/page11_main" />

    <ImageView
        android:id="@+id/page11_img_wall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="480dp"
        android:layout_marginTop="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/page11_btn_wall" />

    <Button
        android:id="@+id/page11_btn_wall"
        android:layout_width="245dp"
        android:layout_height="50dp"
        android:layout_gravity="right"
        android:layout_marginRight="480dp"
        android:layout_marginTop="50dp"
        android:alpha="0" />

    <ImageView
        android:id="@+id/page11_img_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="350dp"
        android:layout_marginTop="480dp"
        android:scaleType="fitXY"
        android:src="@drawable/page11_btn_bord_tv" />

    <Button
        android:id="@+id/page11_btn_tv"
        android:layout_width="290dp"
        android:layout_height="50dp"
        android:layout_gravity="right"
        android:layout_marginRight="350dp"
        android:layout_marginTop="560dp"
        android:alpha="0" />

    <ImageView
        android:id="@+id/page11_img_living_stair"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="58dp"
        android:layout_marginTop="542dp"
        android:scaleType="fitXY"
        android:src="@drawable/page11_btn_all_living_stair" />

    <Button
        android:id="@+id/page11_btn_living_stair"
        android:layout_width="270dp"
        android:layout_height="75dp"
        android:layout_gravity="right"
        android:layout_marginRight="58dp"
        android:layout_marginTop="542dp"
        android:alpha="0" />

    <ImageView
        android:id="@+id/page11_img_pattern"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="350dp"
        android:layout_marginTop="630dp"
        android:scaleType="fitXY"
        android:src="@drawable/page11_btn_tv_pattern" />

    <ImageView
        android:id="@+id/page11_image_hiden"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="65dp"
        android:layout_marginTop="639dp"
        android:scaleType="fitXY"
        android:src="@drawable/page10_btn_hidden" />

    <Button
        android:id="@+id/page11_btn_hiden"
        android:layout_width="265dp"
        android:layout_height="45dp"
        android:layout_gravity="right"
        android:layout_marginRight="65dp"
        android:layout_marginTop="639dp"
        android:alpha="0" />

</FrameLayout>